
<scroll-view
    scroll-y
    class="wrap class-external">
    <view class="cell {{numCurrIndex === index && 'active'}}"
          bindtap="handleTap"
          data-index="{{index}}"
          data-item="{{item}}"
          wx:for="{{data}}"
          wx:key="index">
        <text>{{item.name}}</text>
    </view>
</scroll-view>
